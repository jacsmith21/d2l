{% extends 'dashboard.twig' %}

{% block title %}
    <title>D2L++</title>
{% endblock %}

{% block content %}
    <!--Table-->
    <table class="table">
       <!--Table head-->
       <thead class = "blue-grey lighten-4">
           <tr>
               <th>#</th>
               <th>First Name</th>
               <th>Last Name</th>
               <th>Username</th>
           </tr>
        </thead>
        <tbody>
            {% for deliverable in deliverables %}
                <form method="post" action="/deliverables/{{deverable.id}}" enctype="multipart/form-data">
                    <tr>
                        <td>{{deliverable.name}}</td>
                        <td>Otto</td>
                        <td>@mdo</td>
                    </tr>
                </form>
            {% end for %}
        </tbody>
    </table>
{% endblock %}